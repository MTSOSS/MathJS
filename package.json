{
    "name": "@cypher_077/math",
    "version": "1.1.6",
    "description": "A JavaScript library for basic and advanced arithmetic operations, Satistical functions, logical functions, factorial and fibonacci functions, random number functions, and trigonometric functions.",
    "main": "math.js",
    "types": "math.d.ts",
    "scripts": {
        "build": "tsc && npm run minify",
        "version": "node ./scripts/SemVer.js",
        "test": "node ./tests/NodeTest.js && exit 0",
        "minify": "minify math.js --out-file dist/math.min.js --mangle.keepClassName",
        "prepublish": "npm run version && npm run build",
        "publish": "npm publish --access public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SatyamV7/MathJS.git"
    },
    "keywords": [
        "math",
        "mathjs",
        "math library",
        "math operations",
        "mathematical functions",
        "mathematical operations",
        "arithmetic operations",
        "logical functions",
        "statistical functions",
        "factorial and fibonacci functions",
        "random number functions",
        "trignometric functions"
    ],
    "author": "Satyam Verma",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/MTSOSS/MathJS/issues"
    },
    "homepage": "https://mtsoss.github.io/MathJS/",
    "devDependencies": {
        "babel-minify": "^0.5.2",
        "typescript": "^5.4.5"
    }
}