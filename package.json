{
    "$schema": "https://json.schemastore.org/package",
    "name": "arithmetic-core.js",
    "version": "1.0.7",
    "description": "A JavaScript library for basic and advanced arithmetic operations, comparison functions, factorial and fibonacci functions, random number functions, and trigonometric functions",
    "main": "math.js",
    "types": "math.d.ts",
    "scripts": {
        "test": "node ./tests/NodeTest.js && exit 0",
        "prepublishOnly": "tsc && minify math.js --out-file dist/math.min.js --mangle.keepClassName",
        "publish": "npm publish"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MTSOSS/MathJS.git"
    },
    "keywords": [
        "math",
        "mathjs",
        "math library",
        "math operations",
        "mathematical functions",
        "mathematical operations",
        "arithmetic operations",
        "comparison functions",
        "random number functions",
        "trignometric functions"
    ],
    "author": "Satyam Verma",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/MTSOSS/MathJS/issues"
    },
    "homepage": "https://mtsoss.github.io/MathJS/docs/",
    "devDependencies": {
        "babel-minify": "^0.5.2",
        "typescript": "^5.4.5"
    }
}
